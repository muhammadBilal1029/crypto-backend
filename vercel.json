{
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": "src/**/*",
          "buildCommand": "npm run build"

        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/server.ts"
      }
    ]
  }
  